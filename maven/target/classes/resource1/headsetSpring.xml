<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->

	<bean class="com.xworkz.headset.bean.HeadSet" id="allArgs">
	</bean>


	<bean class="com.xworkz.headset.bean.HeadSet" id="sony">
		<constructor-arg value="Sony" type="java.lang.String"></constructor-arg>
	</bean>

	<bean  class="com.xworkz.headset.bean.HeadSet" id="Red" >
		<constructor-arg value="Red" type="java.lang.String"></constructor-arg>
		<constructor-arg value="true" type="boolean"></constructor-arg>
	</bean>

	<bean class="com.xworkz.headset.bean.HeadSet" id="true">
		<constructor-arg value="true" type="boolean"></constructor-arg>
		<constructor-arg value="2500" type="int"></constructor-arg>
	</bean>

	<bean class="com.xworkz.headset.bean.HeadSet" id="good">
		<constructor-arg value="good" type="java.lang.String"></constructor-arg>
		<constructor-arg value="plastic" type="java.lang.String">
		</constructor-arg>
	</bean>


	<bean class="com.xworkz.headset.bean.HeadSet" id="Rubber">
		<constructor-arg value="Rubber" type="java.lang.String"></constructor-arg>
		<constructor-arg value="Wried" type="java.lang.String"></constructor-arg>
		<constructor-arg value="Over Ear" type="java.lang.String"></constructor-arg>
	</bean>




</beans>       